#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int n1,n2 ,resa,resd;
    float x1,x2,ressa,ressd;
    scanf("%d%d",&n1,&n2);
    scanf("%f%f",&x1,&x2);
    resa= n1+n2;
    resd=n1-n2;
    ressa=x1+x2;
    ressd=x1-x2;
    printf("%d %d\n",resa,resd);
    printf("%0.1f %0.1f",ressa,ressd);
    return 0;
}
